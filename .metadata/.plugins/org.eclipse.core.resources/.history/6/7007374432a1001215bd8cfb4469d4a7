
public class reverse {

	static void reverse(char[] string){
		int stat=0;
		int end=string.length-2;
		while(stat<end){
			char temp;
			temp=string[stat];
			string[stat]=string[end];
			string[end]=temp;
			stat++;
			end--;
		}
	}
	
	public static void main(String[] args){
		char[]={'a','b','c','d',' '};
	}
}
